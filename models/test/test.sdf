      </sensor>
    </link>  
    
    
    <joint name="kinect_joint" type="revolute">
      <child>kinect</child>
      <parent>world</parent>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <upper>0</upper>
          <lower>0</lower>
        </limit>
      </axis>
    </joint>

  </model>
</sdf>
