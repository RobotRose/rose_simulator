          <depthImageCameraInfoTopicName>/kinect_sensor/depth/camera_info</depthImageCameraInfoTopicName>
          <pointCloudTopicName>/kinect_sensor/depth/points</pointCloudTopicName>
          <frameName>kinect</frameName>
          <pointCloudCutoff>0.5</pointCloudCutoff>
          <distortionK1>0.00000000</distortionK1>
          <distortionK2>0.00000000</distortionK2>
          <distortionK3>0.00000000</distortionK3>
          <distortionT1>0.00000000</distortionT1>
          <distortionT2>0.00000000</distortionT2>
          <CxPrime>0</CxPrime>
          <Cx>0</Cx>
          <Cy>0</Cy>
          <focalLength>0</focalLength>
          <hackBaseline>0</hackBaseline>
        </plugin>
         <!--
        <plugin name="kinect_controller" filename="libgazebo_ros_openni_kinect.so">
